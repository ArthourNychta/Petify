<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/FFD368"
    tools:context=".HomePage.HomePage">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Find Nearby Vet!"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="253dp"
        tools:layout_editor_absoluteY="602dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pet Accessories"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="602dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pet Food"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="277dp"
        tools:layout_editor_absoluteY="391dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="83dp"
        android:layout_height="60dp"
        app:srcCompat="@drawable/homeicon"
        tools:layout_editor_absoluteX="170dp"
        tools:layout_editor_absoluteY="38dp"></ImageView>

    <TextView
        android:id="@+id/welcometext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Welcome, Guest!"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageButton
        android:id="@+id/btn_petfood"
        style="@android:style/Widget.Holo.ImageButton"
        android:layout_width="165dp"
        android:layout_height="159dp"
        android:layout_marginEnd="4dp"
        android:outlineProvider="none"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_gettoknowyourpet"
        app:srcCompat="@drawable/petfood"
        tools:layout_editor_absoluteY="219dp" />

    <ImageButton
        android:id="@+id/btn_petaccessories"
        style="@android:style/Widget.Holo.ImageButton"
        android:layout_width="165dp"
        android:layout_height="159dp"
        android:layout_marginBottom="56dp"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.304"
        app:srcCompat="@drawable/petaccessories"
        tools:layout_editor_absoluteX="39dp" />

    <ImageButton
        android:id="@+id/btn_findnearbyvet"
        style="@android:style/Widget.Holo.ImageButton"
        android:layout_width="165dp"
        android:layout_height="159dp"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_petaccessories"
        app:layout_constraintTop_toBottomOf="@+id/btn_petfood"
        app:layout_constraintVertical_bias="0.265"
        app:srcCompat="@drawable/findnearbyvet" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get To Know Your Pet!"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="391dp" />

    <ImageButton
        android:id="@+id/btn_gettoknowyourpet"
        style="@android:style/Widget.Holo.ImageButton"
        android:layout_width="165dp"
        android:layout_height="159dp"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/gettoknowyourpet"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="219dp" />

</androidx.constraintlayout.widget.ConstraintLayout>